package interviewProgram;

public class CountPatternFromString {

	public static void main(String[] args)
	{
		String s ="abcdefabcdefxyzdef";
		String pattern="abc";
		String s1="";
		String arr[]= new String(s.length());
		
		int end = 3;
		for(int start=0; end<s.length(); start++)
		{
			if(s.equals(pattern))
			{
				s1=s1+pattern;
			}
		}
		
	}

}
