package ishwar_practice_star;

public class Star_Pattern_4 {

	public static void main(String[] args) 
	{
		for(int i=1; i<=5; i++)
		{
			for(int j=1; j<=5; j++)
			{
				if(j>=1 && j<=6-i)	
				{
					System.out.print("*");
				}
				else
				{
					System.out.println(" ");
				}
			}
			System.out.println();
		}

	}

}
